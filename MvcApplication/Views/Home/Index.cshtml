@{
    ViewBag.Title = "主页";
}

<!DOCTYPE HTML>
<html lang="en-US">
<head>
    <meta charset="utf-8">
    <title>Gllue</title>
    <meta charset="UTF-8">
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- style -->
    
    <link rel="stylesheet" type="text/css" href="/assets/release/1374761568022/css/main.css">
    
</head>

<body data-weave="app/widget/root/main" class="sidebar_hidden">
    <div id="maincontainer" class="clearfix">
        <header class="navbar navbar-fixed-top" data-weave="app/widget/application app/widget/header/main"></header>

<!-- main content start -->
<div id="contentwrapper">
    <div class="main_content" data-weave="troopjs-core/route/placeholder(app/widget/page-route/main)" data-route=".?"></div>
    <div class="attachment-helper hide open max" data-weave="app/widget/common/attachment-helper/main"></div>
</div>
<!-- main content end -->

<!-- sidebar start -->
<div data-weave="app/widget/side-bar/main('candidate')"></div>
<!-- sidebar end -->

<!-- modals start -->
<div class="modal hide fade" data-weave="app/widget/modal/add-note/main"></div>
<div class="modal modal-email hide fade" data-weave="app/widget/modal/send-email/main"></div>
<div class="modal hide fade" data-weave="app/widget/modal/add-task/main"></div>
<div class="modal hide fade" data-weave="app/widget/modal/add-shortlist/main"></div>
<div class="modal hide fade" data-weave="app/widget/modal/auto-share/main"></div>
<div class="modal hide fade" data-weave="app/widget/modal/add-to-folder/main"></div>
<div class="modal hide fade" id="attachmentModal" data-weave="app/widget/modal/add-attachments/main"></div>
<!-- modals end -->

    </div>
    
    <script type="text/javascript">
        window.config = {
            shareType: 'everyone',
            plugins: ['importer', 'oneclick', 'recommendation', 'linkedin']
        }
    </script>

    
    <script type="text/javascript" data-main="/assets/release/1374761568022/js/main.js" src="/assets/libs/requirejs/require.min.js"></script>
    <script type="text/javascript" src="/assets/release/1374761568022/js/app-built.min.js"></script>
    
</body>
</html>